alias h="cd ~/ && ls -l" \
    d="cd ~/Documents && ls -l" \
    de="cd ~/Desktop && ls -l" \
    D="cd ~/Downloads && ls -l" \
    R="cd ~/Repositories && ls -l" \
    cf="cd ~/.config && ls -l" \
    sc="cd ~/.local/bin && ls -l" \
    dt="cd ~/.local/share && ls -l" \
    fr="cd ~/Sites/frankology && ls -l" \
    tb="cd ~/Testbench && ls -l" \
    cfb="$EDITOR ~/.config/bash/bprofile" \
    cfbr="$EDITOR ~/.config/bash/bashrc" \
    cfz="$EDITOR ~/.config/zsh/.zprofile" \
    cfzr="$EDITOR ~/.config/zsh/.zshrc" \
    cfa="$EDITOR ~/.config/aliasrc" \
    cfs="$EDITOR ~/.config/shortcutrc" \
    cfv="$EDITOR ~/.config/nvim/init.vim" \
    cft="$EDITOR ~/.config/tmux/tmux.conf" \
    cfm="$EDITOR ~/.config/mutt/muttrc" \
    cfw="$EDITOR ~/.config/yabai/yabairc" \
    cfk="$EDITOR ~/.config/skhd/skhdrc" \
